AA,YAAA,CAAA,MAAA;AACA;AATA;AAXA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <notifications\n      group=\"addToCart\"\n      position=\"bottom right\"></notifications>\n    <notifications\n      group=\"UserSave\"\n      position=\"top center\"></notifications>\n    <v-app-bar\n      app\n      color=\"deep-purple\"\n      dark\n    >\n      <v-toolbar-title>\n        <v-btn @click=\"$router.push({ name: 'home' })\" text x-large>SHOPLATE</v-btn>\n      </v-toolbar-title>\n      <v-spacer></v-spacer>\n      <v-menu\n        :close-on-content-click=\"false\"\n        :nudge-width=\"200\"\n        offset-y\n        v-model=\"menu\"\n      >\n        <template v-slot:activator=\"{ on }\">\n          <v-btn small text v-on=\"on\">\n            <v-icon small>mdi-account</v-icon>\n            {{isLogged ? 'olá ' + userName :'faça seu login'}}\n          </v-btn>\n        </template>\n        <login-menu @close-menu=\"menu=false\" v-if=\"!isLogged\"></login-menu>\n        <logged-menu @close-menu=\"menu=false\" v-else></logged-menu>\n      </v-menu>\n      <v-menu\n        :close-on-content-click=\"false\"\n        :nudge-width=\"400\"\n        offset-y\n        v-model=\"cartMenu\"\n      >\n        <template v-slot:activator=\"{ on }\">\n          <v-badge\n            color=\"green\"\n            left\n            overlap\n          >\n            <template v-slot:badge>\n              <span v-if=\"shoppingCartCount > 0\">{{ shoppingCartCount }}</span>\n            </template>\n            <v-btn @click=\"cartMenu=true\" icon v-on=\"on\">\n              <v-icon>mdi-cart</v-icon>\n            </v-btn>\n          </v-badge>\n        </template>\n        <cart-menu @close-menu=\"cartMenu=false\"></cart-menu>\n      </v-menu>\n    </v-app-bar>\n    <v-content>\n      <router-view></router-view>\n    </v-content>\n  </v-app>\n</template>\n\n<script>\nimport LoginMenu from './components/LoginMenu.vue'\nimport LoggedMenu from './components/LoggedMenu.vue'\nimport CartMenu from './components/CartMenu.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    LoginMenu,\n    LoggedMenu,\n    CartMenu\n  },\n  data: () => ({\n    menu: false,\n    cartMenu: false\n  }),\n  computed: {\n    isLogged () {\n      return this.$store.state.isLogged\n    },\n    userName () {\n      return this.$store.state.userInfo.name.split(' ')[0]\n    },\n    shoppingCartCount () {\n      return this.$store.state.shoppingCart.length\n    }\n  }\n}\n</script>\n<style scoped>\n  .v-badge--overlap.v-badge--bottom .v-badge__badg {\n    bottom: 0;\n  }\n</style>\n"],"sourceRoot":"src"}]}